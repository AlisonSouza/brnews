    :javascript
      $.get('http://landing.brnews.co/appetizers',function(data){
        data = JSON.parse(data)
        for(var i=0;i<data.length;i++){
          var row = $('#appetizer'+i)

          if(!row)break

          row.attr('style','')
          var truncated = data[i].title
          if(truncated.length>50) truncated = (truncated.substring(0,85) + " ...")
          row.children('h5').text(truncated)

          var topicn=''
          if(data[i].topic_id==2) topicn='Política'
          else if(data[i].topic_id==3) topicn='Economia'
          else if(data[i].topic_id==4) topicn='Esporte'
          else if(data[i].topic_id==5) topicn='Tecnologia'
          else if(data[i].topic_id==6) topicn='Negócios'

          var small = row.children('small')
          small.children('.topic').text(topicn)
          small.children('.source').text(data[i].fb_from_name)
          small.children('.ago').text(when(data[i].published))

          row.fadeIn( 500, function() { $(this).attr('style','');$(this).removeClass('hide') });
        }

        $('#loadingfeed').hide()
      })
